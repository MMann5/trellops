function boardReducer(state = initialState, action) {
  var newState = state;

  switch (action.type) {
    case 'SET_BOARDS':
      newState = { ...state,
        boards: action.boards
      };
      break;

    default:
      return state;
  } // For debug:


  window.carState = newState;
  console.log('Prev State:', state);
  console.log('Action:', action);
  console.log('New State:', newState);
  return newState;
}

